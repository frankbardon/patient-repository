{
    "name": "accard/patient-repository",
    "license": "MIT",
    "type": "library",
    "description": "Accard Patient Repository",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Frank Bardon Jr.",
            "homepage": "http://frankbardon.me"
        },
        {
            "name": "Database and Applications Group",
            "homepage": "http://dag.med.upenn.edu"
        }
    ],
    "autoload": {
        "psr-4": {
            "Accard\\Bridge\\Twig\\": "src/Bridge/Twig/",
            "Accard\\Bundle\\": "src/Bundle/",
            "Accard\\Component\\": "src/Component/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "dag/framework": "~1.0"
    },
    "require-dev": {
        "codeception/codeception":               "dev-master",
        "codeception/mockery-module":            "dev-master",
        "friendsofsymfony/jsrouting-bundle":     "~2.0@dev",
        "knplabs/knp-menu-bundle":               "~2.0",
        "symfony/symfony":                       "~2.7",
        "symfony/assetic-bundle":                "~2.6",
        "symfony/monolog-bundle":                "~2.7",
        "sensio/distribution-bundle":            "~3.0",
        "sensio/framework-extra-bundle":         "~3.0",
        "doctrine/dbal":                         "~2.5",
        "doctrine/orm":                          "~2.4",
        "doctrine/doctrine-bundle":              "~1.4",
        "doctrine/doctrine-cache-bundle":        "~1.0",
        "doctrine/doctrine-migrations-bundle":   "~1.0",
        "twig/extensions":                       "~1.2",
        "friendsofsymfony/rest-bundle":          "~1.5",
        "jms/serializer-bundle":                 "0.13.*",
        "white-october/pagerfanta-bundle":       "~1.0",
        "lexik/translation-bundle":              "~2.1",
        "gedmo/doctrine-extensions":             "~2.4",
        "willdurand/hateoas-bundle":             "~0.5"
    },
    "suggest": {
        "friendsofsymfony/jsrouting-bundle": "Required when using UIBundle and OutcomesBundle.",
        "knplabs/knp-menu-bundle": "Required when using WebBundle administration."
    },
    "replace": {
        "accard/activity": "self.version",
        "accard/activity-bundle": "self.version",
        "accard/attribute": "self.version",
        "accard/attribute-bundle": "self.version",
        "accard/behavior": "self.version",
        "accard/behavior-bundle": "self.version",
        "accard/core": "self.version",
        "accard/core-bundle": "self.version",
        "accard/diagnosis": "self.version",
        "accard/diagnosis-bundle": "self.version",
        "accard/drug": "self.version",
        "accard/drug-bundle": "self.version",
        "accard/patient": "self.version",
        "accard/patient-bundle": "self.version",
        "accard/phase": "self.version",
        "accard/phase-bundle": "self.version",
        "accard/regimen": "self.version",
        "accard/regimen-bundle": "self.version",
        "accard/sample": "self.version",
        "accard/sample-bundle": "self.version"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev",
            "1.0": "1.0.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        { "type": "composer", "url": "http://accard-dev.med.upenn.edu/satis" }
    ]
}
