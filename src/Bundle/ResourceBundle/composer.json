{
    "name": "accard/resource-bundle",
    "type": "symfony-bundle",
    "description": "Accard resource component",
    "keywords": ["resource", "storage", "persistence", "accard"],
    "homepage": "http://accard.med.upenn.edu",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Bardon Jr.",
            "homepage": "http://frankbardon.com"
        },
        {
            "name": "Database and Applications Group",
            "homepage": "http://dag.med.upenn.edu"
        }
    ],
    "require": {
        "php": ">=5.3.3",

        "symfony/framework-bundle":        "~2.3",
        "symfony/twig-bundle":             "~2.3",
        "symfony/expression-language":     "~2.4",
        "symfony/form":                    "~2.3",
        "accard/resource":                 "dev-develop",
        "friendsofsymfony/rest-bundle":    "~1.0",
        "jms/serializer-bundle":           "0.12.*",
        "white-october/pagerfanta-bundle": "1.0.*",
        "doctrine/doctrine-bundle":        "~1.3@dev"
    },
    "require-dev": {
        "twig/twig":            "~1.11",
        "doctrine/orm":         "~2.3"
    },
    "suggest": {
        "fsc/hateoas-bundle":   "0.3.*@dev",
        "doctrine/orm":         "~2.3",
        "doctrine/mongodb-odm": "1.0.*@dev"
    },
    "autoload": {
        "psr-4": { "Accard\\Bundle\\ResourceBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Accard\\Bundle\\ResourceBundle\\spec\\": "spec/" }
    }
}