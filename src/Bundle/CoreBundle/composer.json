{
    "name": "accard/core-bundle",
    "type": "symfony-bundle",
    "description": "Accard core bundle",
    "keywords": ["core"],
    "homepage": "http://accard.med.upenn.edu",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Bardon Jr.",
            "homepage": "http://frankbardon.com"
        },
        {
            "name": "Database and Applications Group",
            "homepage": "http://dag.med.upenn.edu"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "accard/core":              "~1.0",
        "accard/resource-bundle":   "~1.0",
        "accard/activity-bundle":   "~1.0"
    },
    "require-dev": {
        "codeception/codeception":          "dev-master",
        "codeception/mockery-module":       "dev-master"
    },
    "suggest": {
        "accard/web-bundle": "Provides the default web interface",
        "accard/fixtures-bundle": "Provides the default Accard fixtures"
    },
    "autoload": {
        "psr-4": { "Accard\\Bundle\\CoreBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Accard\\Bundle\\CoreBundle\\spec\\": "spec/" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        { "type": "composer", "url": "http://accard-dev.med.upenn.edu/satis" }
    ]
}
