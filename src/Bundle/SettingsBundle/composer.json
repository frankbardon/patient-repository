{
    "name": "accard/settings-bundle",
    "type": "symfony-bundle",
    "description": "Accard settings bundle",
    "keywords": ["accard", "settings"],
    "homepage": "http://accard.med.upenn.edu",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Bardon Jr.",
            "homepage": "http://frankbardon.com"
        },
        {
            "name": "Dylan Pierce",
            "homepage": "http://dylanjpierce.com"
        },
        {
            "name": "Database and Applications Group",
            "homepage": "http://dag.med.upenn.edu"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "accard/resource-bundle":      "~1.0"
    },
    "require-dev": {
        "codeception/codeception":          "dev-master",
        "codeception/mockery-module":       "dev-master"
    },
    "autoload": {
        "psr-4": { "Accard\\Bundle\\SettingsBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Accard\\Bundle\\SettingsBundle\\spec\\": "spec/" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        { "type": "composer", "url": "http://accard-dev.med.upenn.edu/satis" }
    ]
}
