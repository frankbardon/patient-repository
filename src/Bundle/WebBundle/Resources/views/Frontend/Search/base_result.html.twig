<section class="search-result">
{% block result %}
    <h4>
        <a href="{% block search_link '#' %}" class="search-link">{% block search_text 'Overwrite' %}</a>
        <small class="search-score">
            {% block search_score %}score {{ result.percentage }}%{% endblock %}
        </small>
    </h4>
    <p>{% block search_description '' %}</p>
{% endblock %}
</section>
