{% extends 'AccardWebBundle:Frontend/Patient:index.html.twig' %}

{% from 'AccardWebBundle:Common/Macros:misc.html.twig' import pagination %}
{% from 'AccardWebBundle:Frontend/Patient:macros.html.twig' import import_list %}

{% block patient_content %}
    {{ import_list(patients) }}
    {{ pagination(patients) }}
{% endblock %}

{% block page_javascripts %}
    {% javascripts output='assets/compiled/import.js' 'bundles/accardresource/js/import.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script type="text/javascript">
        $('tr[data-toggle="popover"]').popover();
    </script>
{% endblock %}