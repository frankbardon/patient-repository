{% from 'AccardWebBundle:Common/Macros:form.html.twig' import select_patient %}


<div class="row">
{% if form.patient is defined %}
    {{ select_patient(form.patient) }}
{% endif %}
</div>

<hr>
<div class="col-lg-3">
    <h3>Create Behavior</h3>
    <p>Create the behavior for the selected patient.</p>
</div>
<div class="col-lg-9">
    <div class="well">

            {{form_row(form.startDate)}}
            
            {{form_row(form.endDate)}}

            {% block behavior_form_extra_fields %}
            {% endblock %}

            
    </div>
</div>

