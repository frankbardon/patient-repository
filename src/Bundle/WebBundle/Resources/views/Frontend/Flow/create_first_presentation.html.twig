{% extends 'AccardWebBundle:Frontend/Flow:layout.html.twig' %}

{% from 'AccardWebBundle:Common/Macros:misc.html.twig' import page_title %}

{% block flow_content %}
    {{ page_title('accard.diagnosis.action.create'|trans) }}
    {{ form_errors(form) }}    

	<div class="row">
	    <div class="col-md-6">
	        {{ form_row(form.code) }}
	        {{ form_row(form.startDate) }}

	    </div>
	    <div class="col-md-6">
	        {% for field in form.fields %}
	            {{ form_widget(field) }}
	        {% endfor %}
	    </div>
	</div>

{% endblock %}

