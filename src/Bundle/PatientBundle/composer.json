{
    "name": "accard/patient-bundle",
    "type": "symfony-bundle",
    "description": "Accard patient bundle",
    "keywords": ["accard", "patient"],
    "homepage": "http://accard.med.upenn.edu",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Bardon Jr.",
            "homepage": "http://frankbardon.com"
        },
        {
            "name": "Database and Applications Group",
            "homepage": "http://dag.med.upenn.edu"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "accard/resource-bundle":   "dev-develop",
        "accard/patient":           "dev-develop"
    },
    "autoload": {
        "psr-4": { "Accard\\Bundle\\PatientBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Accard\\Bundle\\PatientBundle\\spec\\": "spec/" }
    },
    "repositories": [
        { "type": "vcs", "url": "git@gitlab.med.upenn.edu:prototype/Resource.git" },
        { "type": "vcs", "url": "git@gitlab.med.upenn.edu:prototype/ResourceBundle.git" },
        { "type": "vcs", "url": "git@gitlab.med.upenn.edu:prototype/Patient.git" }
    ]
}
