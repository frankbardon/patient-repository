{
    "name": "accard/sample",
    "type": "library",
    "description": "Accard sample component",
    "keywords": ["accard", "sample"],
    "homepage": "http://accard.med.upenn.edu",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "dag/resource": "~1.0",
        "dag/prototype": "~1.0"
    },
    "require-dev": {
        "codeception/codeception": "dev-master",
        "codeception/mockery-module": "dev-master"
    },
    "autoload": {
        "psr-4": { "Accard\\Component\\Sample\\": "" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
       "bin-dir": "bin"
    },   
    "repositories": [
        { "type": "composer", "url": "http://accard-dev.med.upenn.edu/satis" }
    ]
}
