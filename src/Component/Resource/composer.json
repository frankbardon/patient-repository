{
    "name": "accard/resource",
    "type": "library",
    "description": "Accard resource component",
    "keywords": ["accard", "resource"],
    "homepage": "http://accard.med.upenn.edu",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "doctrine/common": "~2.4",
        "symfony/security": "~2.7"
    },
    "require-dev": {
        "codeception/codeception": "dev-master",
        "codeception/mockery-module": "dev-master" 
    },  
    "autoload": {
        "psr-4": { "Accard\\Component\\Resource\\": "" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
       "bin-dir": "bin"
    },    
    "repositories": [
        { "type": "composer", "url": "http://accard-dev.med.upenn.edu/satis" }
    ]
}
