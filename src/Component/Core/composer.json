{
    "name": "accard/core",
    "type": "library",
    "description": "Accard core component",
    "keywords": ["accard", "core"],
    "homepage": "http://accard.med.upenn.edu",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "accard/activity": "~1.0",
        "accard/attribute": "~1.0",
        "accard/behavior": "~1.0",
        "accard/diagnosis": "~1.0",
        "accard/patient": "~1.0",
        "accard/phase": "~1.0",
        "accard/regimen": "~1.0",
        "accard/sample": "~1.0"
    },
    "autoload": {
        "psr-4": { "Accard\\Component\\Core\\": "" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require-dev": {
       "codeception/codeception": "dev-master",
       "codeception/mockery-module": "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        { "type": "composer", "url": "http://accard-dev.med.upenn.edu/satis" }
    ]
}
